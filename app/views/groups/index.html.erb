<header class="d-flex justify-content-between align-items-center header">
  <%= link_to image_tag("https://img.icons8.com/officel/344/back.png"),groups_path, class: "back_btn" %>
  <h5>groups</h5>
  <div></div>
</header>
<% if flash[:notice] %>
  <p class="alert alert-primary" role="alert"><%= notice %></p>
<% end %>
<div id="groups">
  <% @groups.each do |group| %>
    <%= render group %>
  <% end %>
</div>
<div class="d-flex justify-content-center fixed-bottom new-btn">
  <%= link_to "New group", new_group_path%>
</div>